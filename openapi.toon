info:
  title: BE Ecomerce Shop
  description: 
  version: 1.0
  contact:
tags: []servers: []components:
  schemas:
    UserEntity:
      type: object
      properties:
        id:
          format: int64
          type: integer
          example: 1
          description: User ID
        firstName:
          type: string
          example: John
          description: First name
        lastName:
          type: string
          example: Doe
          description: Last name
        gender:
          type: string
          enum: items[3]:
              - MALE
              - FEMALE
              - OTHER
          description: Gender
        email:
          type: string
          example: user@example.com
          description: Email address
        phone:
          type: string
          example: 0123456789
          description: Phone number
        username:
          type: string
          example: johndoe
          description: Username
        password:
          type: string
          example: hashedpassword
          description: Password hash
        role:
          type: string
          enum: items[3]:
              - USER
              - ADMIN
              - OPERATOR
          description: User role
        isActive:
          type: boolean
          example: true
          description: Account active status
        codeActive:
          type: string
          example: 123e4567-e89b-12d3-a456-426614174000
          description: Activation code
        codeActiveExpire:
          format: date-time
          type: string
          example: 2024-12-31T23:59:59Z
          description: Activation code expiry
        points:
          type: number
          example: 100
          description: Loyalty points
        createdAt:
          format: date-time
          type: string
          example: 2024-01-01T00:00:00Z
          description: Created at
        updatedAt:
          format: date-time
          type: string
          example: 2024-01-01T00:00:00Z
          description: Updated at
        staffCode:
          type: string
          example: STF001
          description: Staff code
        isAdmin:
          type: boolean
          example: false
          description: Is admin
        shopOfficeId:
          format: int64
          type: integer
          example: 1
          description: Shop office ID
        loyaltyCard:
          type: string
          example: LOYAL123
          description: Loyalty card number
      required: items[12]:
          - id
          - email
          - username
          - password
          - role
          - isActive
          - codeActive
          - codeActiveExpire
          - points
          - createdAt
          - updatedAt
          - isAdmin
    CreateUserDto:
      type: object
      properties:
        firstName:
          type: string
          example: John
        lastName:
          type: string
          example: Doe
        gender:
          type: string
          example: MALE | FEMALE | OTHER
        email:
          type: string
          example: john.doe@example.com
        phone:
          type: string
          example: 1234567890
        password:
          type: string
          example: password123
        username:
          type: string
          example: johndoe
        role:
          type: string
          example: USER | ADMIN | OPERATOR
        createdAt:
          format: date-time
          type: string
          example: 2025-12-04T07:22:41.360Z
        isActive:
          type: boolean
          example: false
        codeActive:
          type: string
          example: false
        codeActiveExpire:
          format: date-time
          type: string
          example: 2025-12-04T07:22:41.360Z
